{
    "dependencies": {
        "html-minifier": ">=3.0.1",
        "pug-cli": ">=1.0.0-alpha6"
    },
    "description": "Legal notes for my documentation-, home- and websites.",
    "files": ["index.pug", "htmlMinifierConfig.json"],
    "homepage": "http://torben.website/legalNotes",
    "keywords": [
        "legal", "notes", "about", "website", "homepage", "impressum"
    ],
    "license": "ISC",
    "main": "index.html",
    "name": "legalNotes",
    "repository": {
        "type": "git", "url": "https://github.com/thaibault/legalNotes.git"
    },
    "scripts": {
        "build": "pug --no-debug --extension pug.html index.pug && html-minifier --config-file htmlMinifierConfig.json --output index.html index.pug.html",
        "buildPretty": "pug --pretty index.pug",
        "clear": "rm *.html || true",
        "preinstall": "touch index.html",
        "postinstall": "npm run build",
        "watch": "pug --watch index.pug"
    },
    "version": "1.0.0"
}
