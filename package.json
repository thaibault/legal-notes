{
  "name": "legal-notes",
  "version": "1.0.3",
  "license": "CC-BY-3.0",
  "description": "Legal notes for documentation-, home- and websites.",
  "homepage": "http://torben.website/legalNotes",
  "keywords": [
    "legal",
    "notes",
    "about",
    "website",
    "homepage",
    "impressum"
  ],
  "files": [
    "index.html"
  ],
  "main": "index.html",
  "repository": {
    "type": "git",
    "url": "https://github.com/thaibault/legalNotes.git"
  },
  "scripts": {
    "build": "pug --no-debug --extension pug.html index.pug && html-minifier --config-file htmlMinifierConfig.json --output index.html index.pug.html",
    "buildPretty": "pug --pretty index.pug",
    "clear": "rm *.html || true",
    "prepublish": "npm run build",
    "update": "npm version patch && npm publish",
    "watch": "pug --watch index.pug"
  },
  "devDependencies": {
    "html-minifier": "latest",
    "pug-cli": "latest"
  }
}
